n  = int(input())
nums = list(map(int , input().split()))
count = 0


nums.sort(reverse=True)

i = 0 
while i < len(nums) and n >= 1:
    
    if nums[i] >= n:
        count += 1
        # nums.remove(nums[i])
        n -= 1
        i += 1
        
    elif i == len(nums) - 1 and nums[i] < n:
        n -= 1
    else:
        # i += 1
        n -= 1
        '''
for i in sorted(nums):
    if i >= n:
        count += 1
        n -= 1
    else:
        n -= 1

'''
    
print(count)

    
