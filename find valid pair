class Solution:
    from collections import Counter
    def findValidPair(self, s: str) -> str:
        counter_dict = Counter(s)
        for i in range(len(s)-1):
           
            if s[i] != s[i+1] and counter_dict[s[i]] == int(s[i]) and counter_dict[s[i+1]]==int(s[i+1]):
                return s[i]+s[i+1]
        return ""
