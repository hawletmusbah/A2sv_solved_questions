class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        minadd = float('inf')
        add= 0
        listt = []
        listnew =[]
        for index1,value1 in enumerate(list1):
            for index2,value2 in enumerate(list2):
                if value1 == value2:
                    add = index1+index2
                    if add<minadd:
                        minadd = add
                        listnew=[value1]
                        listt=copy.copy(listnew)
                        listnew.clear()
                    elif add==minadd:
                        listt.append(value1)


        return listt

        
